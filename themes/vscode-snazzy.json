// foregroundColor / black = '#eff0eb'
// backgroundColor / white = '#282a36'
// cursorColor: '#97979b'
// hightlightColor: '#54555e'
// red = '#ff5c57'
// green = '#5af78e'
// yellow = '#f3f99d'
// blue = '#57c7ff'
// magenta = '#ff6ac1'
// cyan = '#9aedfe'
// gray = '#616263'

{
  "name": "Snazzy",
  "type": "dark",
  "tokenColors": [
    // Settings
    {
      "name": "Global settings",
      "settings": {
        "background": "#282a36",
        "foreground": "#eff0eb"
      }
    },
    // General
    {
      "name": "Strong",
      "scope": "strong, markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    // Comments
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#616263",
        "fontStyle": "italic"
      }
    },
    // Storage
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    // Types
    {
      "name": "Type",
      "scope": "storage.type.primitive, support.type, entity.name.type, entity.other.inherited-class, keyword.other.unit",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    // Primitives & Strings
    {
      "name": "String",
      "scope": "string, meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, support.constant",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Undefined",
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    // Functions
    {
      "name": "Function call",
      "scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    // Variables & Constants
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Constants",
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    // Classes
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    // Keywords & Modifiers
    {
      "name": "Keyword",
      "scope": "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    // Modules
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    // Templates
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    // Exceptions
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    // Diffs
    {
      "name": "Diff insertion",
      "scope": "markup.inserted.diff, meta.diff.header.to-file",
      "settings": {
        "foreground": "#5af78e",
        "background": "#616263"
      }
    },
    {
      "name": "Diff deletion",
      "scope": "markup.deleted.diff, meta.diff.header.from-file",
      "settings": {
        "foreground": "#ff5c57",
        "background": "#616263"
      }
    },
    // C#
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#616263"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    // CSS
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "CSS property",
      "scope": "source.css support.type.property-name",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    // HTML & JSX
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "HTML tag expression",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    // INI
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    // Javascript
    {
      "name": "Async",
      "scope": "source.js storage.modifier.async",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    // JSON
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    // Markdown
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference, markup.underline.link.image",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Markdown code",
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic bold"
      }
    },
    // YAML
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#ff5c57"
      }
    }
  ],
  "colors": {
    // Base
    "foreground": "#eff0eb",
    "focusedElementOutline": "#616263",
    "infoBackground": "#57c7ff",
    "infoBorder": "#eff0eb",
    "warningBackground": "#f3f99d",
    "warningBorder": "#eff0eb",
    "errorBackground": "#ff5c57",
    "errorBorder": "#eff0eb",

    // Editor
    "editorLink.activeForeground": "#ff5c57",
    "editor.background": "#282a36",
    "editorCursor.foreground": "#97979b",
    "editor.findMatchHighlightBackground": "#54555e",
    "editor.findRangeHighlightBackground": "#54555e",
    "editor.foreground": "#eff0eb",
    "editor.hoverHighlightBackground": "#54555e",
    "editorIndentGuide.background": "#616263",
    "editor.lineHighlightBorder": "#54555e",
    "editorLineNumber.foreground": "#616263",
    "editor.rangeHighlightBackground": "#54555e",
    "editor.selectionHighlightBackground": "#54555e",
    "editorWhitespace.foreground": "#616263",
    "editor.wordHighlightBackground": "#54555e",
    "editor.wordHighlightStrongBackground": "#54555e",

    // Peek view
    "peekView.border": "#5af78e",
    "peekViewEditor.background": "#282a36",

    // Statusbar
    "statusBar.background": "#333333",
    "statusBar.noFolderBackground": "#333333",

    // Workbench: Terminal
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiBrightBlack": "#282a36",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightGreen": "#5af78e",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightWhite": "#eff0eb",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiGreen": "#5af78e",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiWhite": "#eff0eb",
    "terminal.ansiYellow": "#f3f99d"
  }
}
